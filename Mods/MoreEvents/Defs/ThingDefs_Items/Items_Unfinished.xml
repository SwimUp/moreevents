		<comps>
			<li Class="CompProperties_Forbiddable"/>
			<li Class="MoreEvents.Things.CompProperties_PlacebleItem">
				<PlaceDef>ColdFusionReactor</PlaceDef>
			</li>
		</comps>
		<inspectorTabs>
			<li>ITab_Art</li>
		</inspectorTabs>
