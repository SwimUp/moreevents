	<RecipeDef>
		<defName>SmeltingPalladiumIngot</defName>
		<label>Smelt Palladium Ingots</label>
		<description>Smelting a palladium ingots by combining nickel, silver and copper</description>
		<jobString>Smelts ingots</jobString>
		<workAmount>3000</workAmount>
		<workSpeedStat>SmeltingSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_Smelt</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>NickelIngot</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Silver</li>
					</thingDefs>
				</filter>
				<count>30</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>CopperIngot</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<products>
			<PalladiumIngot>5</PalladiumIngot>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>NickelOre</li>
				<li>Silver</li>
				<li>CopperIngot</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>

